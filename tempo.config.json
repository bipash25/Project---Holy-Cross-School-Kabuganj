{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "new-york",
  "rsc": false,
  "tsx": true,
  "tailwind": {
    "config": "tailwind.config.js",
    "css": "src/index.css",
    "baseColor": "slate",
    "cssVariables": true,
    "prefix": ""
  },
  "storyboards": [
    {
      "name": "Application",
      "path": "/",
      "component": "src/tempobook/storyboards/application/index.tsx",
      "type": "APPLICATION"
    },
    {
      "name": "Preview",
      "path": "/preview",
      "component": "src/tempobook/storyboards/preview/index.tsx",
      "type": "APPLICATION"
    },
    {
      "name": "Home",
      "component": "src/tempobook/storyboards/home/index.tsx",
      "type": "COMPONENT"
    },
    {
      "name": "Header",
      "component": "src/tempobook/storyboards/header/index.tsx",
      "type": "COMPONENT"
    }
  ],
  "aliases": {
    "components": "@/components",
    "utils": "@/lib/utils"
  }
}